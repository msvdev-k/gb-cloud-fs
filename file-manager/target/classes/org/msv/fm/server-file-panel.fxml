<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="1.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.msv.fm.ServerFilePanelController">


    <HBox>
        <ComboBox/>
        <TextField HBox.hgrow="ALWAYS" editable="false"/>
        <Button text="Вверх"/>
    </HBox>

    <ListView fx:id="serverListView" VBox.vgrow="ALWAYS"/>

    <properties>
        <ctrl>
            <fx:reference source="controller"/>
        </ctrl>
    </properties>

</VBox>
